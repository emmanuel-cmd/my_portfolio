var geometry = /* color: #d63000 */ee.Geometry.MultiPoint();
/***** End of imports. If edited, may not auto-convert in the playground. *****/
// Section 6: Other datasets

/////
// Other datasets
/////

// Import and filter a gridded population dataset.
var griddedPopulation = ee.ImageCollection('CIESIN/GPWv411/GPW_Population_Count').first();

// Predefined palette.
var populationPalette = [
'ffffe7',
'86a192',
'509791',
'307296',
'2c4484',
'000066'
];
// Center the Map.
Map.centerObject(griddedPopulation, 3);
// Add the population data to the map.
Map.addLayer(
  griddedPopulation,
    {
      min: 0,
      max: 1200,
      'palette': populationPalette //Lighter colors correspond to lower popn counts while darker colors correspond to higher popn counts
    },
  'Gridded Population'
);

// Import the NASA DEM Dataset.
var nasaDEM = ee.Image('NASA/NASADEM_HGT/001');

// Add the elevation layer to the map.
Map.addLayer(
  nasaDEM, {
    bands: ['elevation'],
    min: 0,
    max: 5000
  }, 
'NASA DEM'
);

// Synthesis

// Import the Global Phenology data
var phenoData = ee.ImageCollection("NOAA/VIIRS/001/VNP22Q2");
var phenoDataGS = phenoData.select('PGQ_Growing_Season_1');

print("Land Surface Phenology Yearly", phenoData)
print("Land Surface Growing Season", phenoDataGS)

Map.addLayer(
  phenoData, {
    bands: ['PGQ_Growing_Season_1'], 
    min: 0,
    max: 255
  }, 
  'PGQ_Growing_Season'
);